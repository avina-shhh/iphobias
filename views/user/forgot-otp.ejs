	<script>

document.addEventListener("DOMContentLoaded", function () {
    const otpInput = document.getElementById("otp");
    const resendOtpButton = document.getElementById("resendOtpButton");
    const verifyOtpButton = document.getElementById("submitOtpButton");
    const countdownTimer = document.getElementById("countdownTimer");

    let timerDuration = 60; // Timer duration in seconds
    let timer;

    // Function to start the countdown timer
    function startTimer() {
        resendOtpButton.disabled = true;
        resendOtpButton.classList.remove("active");
        countdownTimer.textContent = `01:00`;

        timer = setInterval(() => {
            timerDuration--;
            const minutes = Math.floor(timerDuration / 60);
            const seconds = timerDuration % 60;
            countdownTimer.textContent = `${minutes.toString().padStart(2, "0")}:${seconds.toString().padStart(2, "0")}`;

            if (timerDuration <= 0) {
                clearInterval(timer);
                countdownTimer.textContent = "00:00";
                resendOtpButton.disabled = false;
                resendOtpButton.classList.add("active");
            }
        }, 1000);
    }

    // Start the timer on page load
    startTimer();

    // Resend OTP functionality
    resendOtpButton.addEventListener("click", function (event) {
        event.preventDefault();

		if (resendOtpButton.disabled) return;

        // Reset the timer
        timerDuration = 60;
        startTimer();

        // AJAX request to resend OTP
        fetch("/resend-otp", {
            method: "POST",
            headers: {
                "Content-Type": "application/json"
            }
        })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    Swal.fire({
                        icon: "success",
                        title: "Success",
                        text: data.message || "OTP has been resent to your email."
                    });
                } else {
                    Swal.fire({
                        icon: "error",
                        title: "Error",
                        text: data.message || "Failed to resend OTP. Please try again."
                    });
                }
            })
            .catch(error => {
                console.error(error);
                Swal.fire({
                    icon: "error",
                    title: "Error",
                    text: "An unexpected error occurred. Please try again."
                });
            });
    });

    // Enable "Verify OTP" button when OTP input length is 6
    otpInput.addEventListener("input", function () {
        if (otpInput.value.length === 6) {
            verifyOtpButton.disabled = false;
            verifyOtpButton.classList.add("active");
        } else {
            verifyOtpButton.disabled = true;
            verifyOtpButton.classList.remove("active");
        }
    });

    // Verify OTP functionality
    verifyOtpButton.addEventListener("click", function (event) {
        event.preventDefault();

        const otp = otpInput.value.trim();

        // Validation
        if (otp.length !== 6) {
            Swal.fire({
                icon: "error",
                title: "Error",
                text: "Please enter a valid 6-digit OTP."
            });
            return;
        }

        // AJAX request to verify OTP
        fetch("/verify-otp", {
            method: "POST",
            headers: {
                "Content-Type": "application/json"
            },
            body: JSON.stringify({ otp: otp })
        })
            .then(response => response.json())
            .then(data => {
                if (data.success) {
                    Swal.fire({
                        icon: "success",
                        title: "Success",
                        text: data.message || "OTP verified successfully!"
                    }).then(() => {
                        window.location.href = "/change-password";
                    });
                } else {
                    Swal.fire({
                        icon: "error",
                        title: "Error",
                        text: data.message || "Invalid OTP. Please try again."
                    });
                }
            })
            .catch(error => {
                console.error(error);
                Swal.fire({
                    icon: "error",
                    title: "Error",
                    text: "An unexpected error occurred. Please try again."
                });
            });
    });
});

	</script>
