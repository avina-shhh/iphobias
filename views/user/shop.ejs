	<script>
		function loadMore() {
		    const currentLimit = parseInt(document.getElementById('loadMoreBtn').getAttribute('currentLimit'));
		    const newLimit = currentLimit + 4;
		    const urlParams = new URLSearchParams(window.location.search);
		    
		    // Update the limit parameter
		    urlParams.set('limit', newLimit);
		    
		    // Construct the new URL with all parameters properly encoded
		    const newUrl = `/shop?${urlParams.toString()}`;
		    window.location.href = newUrl;
		}
	</script>

	<script>
		function handleSearch(e) {
			e.preventDefault();
			const searchQuery = e.target.search.value;
			const currentUrl = new URL(window.location.href);
			const params = new URLSearchParams(currentUrl.search);
			params.set('search', searchQuery);
			window.location.href = `/shop?${params.toString()}`;
		}
	</script>

	<%
	function getSortUrl(sortType) {
	    const urlParams = new URLSearchParams();
	    if (selectedCategory && selectedCategory !== 'all') {
	        urlParams.set('category', selectedCategory);
	    }
	    if (currentLimit) {
	        urlParams.set('limit', currentLimit);
	    }
		if(selectedBrand && selectedBrand !== 'all'){
			urlParams.set('brand', selectedBrand);
		}
		if(selectedPrice && selectedPrice !== 'all'){
			urlParams.set('price', selectedPrice);
		}
	    urlParams.set('sort', sortType);
	    return '/shop?' + urlParams.toString();
	}
	%>

